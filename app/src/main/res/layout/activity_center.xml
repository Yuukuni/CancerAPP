<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.huangyuwei.myapplication.center">

    <ImageButton
        android:id="@+id/Btn_cure"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toTopOf="@+id/first_row"
        app:layout_constraintBottom_toBottomOf="@+id/first_row"
        app:layout_constraintLeft_toLeftOf="@+id/first_column"
        app:layout_constraintRight_toRightOf="@+id/first_column"
        app:srcCompat="@drawable/btn_cure"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_move"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toTopOf="@+id/first_row"
        app:layout_constraintBottom_toBottomOf="@+id/first_row"
        app:layout_constraintLeft_toLeftOf="@+id/second_column"
        app:layout_constraintRight_toRightOf="@+id/second_column"
        app:srcCompat="@drawable/btn_move"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_eat"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/btn_eat"
        app:layout_constraintTop_toTopOf="@+id/first_row"
        app:layout_constraintBottom_toBottomOf="@+id/first_row"
        app:layout_constraintLeft_toLeftOf="@+id/third_column"
        app:layout_constraintRight_toRightOf="@+id/third_column"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_laugh"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/btn_laugh"
        app:layout_constraintTop_toTopOf="@+id/second_row"
        app:layout_constraintBottom_toBottomOf="@+id/second_row"
        app:layout_constraintLeft_toLeftOf="@+id/first_column"
        app:layout_constraintRight_toRightOf="@+id/first_column"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_mem"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/btn_mem"
        app:layout_constraintTop_toTopOf="@+id/second_row"
        app:layout_constraintBottom_toBottomOf="@+id/second_row"
        app:layout_constraintLeft_toLeftOf="@+id/second_column"
        app:layout_constraintRight_toRightOf="@+id/second_column"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_link"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/btn_link"
        app:layout_constraintTop_toTopOf="@+id/second_row"
        app:layout_constraintBottom_toBottomOf="@+id/second_row"
        app:layout_constraintLeft_toLeftOf="@+id/third_column"
        app:layout_constraintRight_toRightOf="@+id/third_column"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_talk"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/btn_talk"
        app:layout_constraintTop_toTopOf="@+id/third_row"
        app:layout_constraintBottom_toBottomOf="@+id/third_row"
        app:layout_constraintLeft_toLeftOf="@+id/first_column"
        app:layout_constraintRight_toRightOf="@+id/first_column"
        android:background="@null" />

    <ImageButton
        android:id="@+id/Btn_ask"
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/btn_ask"
        app:layout_constraintTop_toTopOf="@+id/third_row"
        app:layout_constraintBottom_toBottomOf="@+id/third_row"
        app:layout_constraintLeft_toLeftOf="@+id/second_column"
        app:layout_constraintRight_toRightOf="@+id/second_column"
        android:background="@null" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="笑吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_laugh"
        app:layout_constraintLeft_toLeftOf="@id/Btn_laugh"
        app:layout_constraintRight_toRightOf="@id/Btn_laugh"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="吃吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_eat"
        app:layout_constraintLeft_toLeftOf="@id/Btn_eat"
        app:layout_constraintRight_toRightOf="@id/Btn_eat"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="治吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_cure"
        app:layout_constraintLeft_toLeftOf="@id/Btn_cure"
        app:layout_constraintRight_toRightOf="@id/Btn_cure"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="動吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_move"
        app:layout_constraintLeft_toLeftOf="@id/Btn_move"
        app:layout_constraintRight_toRightOf="@id/Btn_move"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="問吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_ask"
        app:layout_constraintLeft_toLeftOf="@id/Btn_ask"
        app:layout_constraintRight_toRightOf="@id/Btn_ask"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="記吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_mem"
        app:layout_constraintLeft_toLeftOf="@id/Btn_mem"
        app:layout_constraintRight_toRightOf="@id/Btn_mem"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="說吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_talk"
        app:layout_constraintLeft_toLeftOf="@id/Btn_talk"
        app:layout_constraintRight_toRightOf="@id/Btn_talk"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="連吧"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_link"
        app:layout_constraintLeft_toLeftOf="@id/Btn_link"
        app:layout_constraintRight_toRightOf="@id/Btn_link"
        />

    <android.support.constraint.Guideline
        android:id="@+id/first_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.195" />

    <android.support.constraint.Guideline
        android:id="@+id/second_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.485" />

    <android.support.constraint.Guideline
        android:id="@+id/third_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.775" />

    <android.support.constraint.Guideline
        android:id="@+id/first_column"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.175" />

    <android.support.constraint.Guideline
        android:id="@+id/second_column"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <android.support.constraint.Guideline
        android:id="@+id/third_column"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.825" />

</android.support.constraint.ConstraintLayout>
